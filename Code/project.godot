[application]

config/name="MoS_minimalist"
run/main_scene="res://Scenes/IntroScene.tscn"
config/icon="res://icon.png"

[autoload]

NodeTools="*res://Scripts/Globals/NodeTools.gd"
Enums="*res://Scripts/Globals/Enums.gd"
Globals="*res://Scripts/Globals/Globals.gd"

[display]

window/size/width=1280
window/size/height=800
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input]

PAUSE={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":0,"physical_scancode":16777217,"unicode":0,"echo":false,"script":null)
 ]
}

[layer_names]

2d_physics/layer_1="HUMAN_CITY"
2d_physics/layer_2="NPC_CITY"
2d_physics/layer_3="HUMAN_ATTACK"
2d_physics/layer_4="NPC_ATTACK"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://default_env.tres"
